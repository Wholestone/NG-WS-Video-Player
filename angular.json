{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "full-lib": {
      "projectType": "library",
      "root": "projects/full-lib",
      "prefix": "ws",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/full-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/full-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/full-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "ng-ws-input": {
      "projectType": "library",
      "root": "projects/ng-ws-input",
      "sourceRoot": "projects/ng-ws-input/src",
      "prefix": "ws",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ng-ws-input/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-ws-input/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-ws-input/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "ng-ws-textarea": {
      "projectType": "library",
      "root": "projects/ng-ws-textarea",
      "sourceRoot": "projects/ng-ws-textarea/src",
      "prefix": "ws",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ng-ws-textarea/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-ws-textarea/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-ws-textarea/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "ng-ws-video-player": {
      "projectType": "library",
      "root": "projects/ng-ws-video-player",
      "sourceRoot": "projects/ng-ws-video-player/src",
      "prefix": "ws",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ng-ws-video-player/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-ws-video-player/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-ws-video-player/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ng-ws-video-player/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}
